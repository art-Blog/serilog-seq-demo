{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:vue/essential"
    ],
    "parserOptions": {
        "parser": "babel-eslint",
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "vue"
    ],
    "globals": {
        "defineProps": false,
        "defineEmits": false
    },
    "rules": {
        "vue/no-multiple-template-root": "off",
        "vue/multi-word-component-names": "warn",
        "vue/require-valid-default-prop": "off"
    }
}
